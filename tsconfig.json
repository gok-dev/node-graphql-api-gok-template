{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "dist",
    "baseUrl": ".",
    "sourceMap": true,
    "allowJs": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "include": [
    "app/**/*.ts"
  ]
}
